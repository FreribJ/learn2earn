MariaDBServer 10.10.02

create table user
(
nutzerid INTEGER NOT NULL AUTO_INCREMENT,
name VARCHAR(255),
geburtsdatum DATE,
passwort INTEGER,
einkommen INTEGER,
PRIMARY KEY (nutzerid)
)

create table umsaetze
(
Datum DATE,
budgettype VARCHAR(255),
betrag INTEGER,
nutzerid INTEGER,
beschreibung VARCHAR(255),
PRIMARY KEY(nutzerid)
)

create table budget
(
nutzer-id INTEGER NOT NULL AUTO_INCREMENT,
auto INTEGER,
wohnen INTEGER,
freizeit INTEGER,
essen INTEGER,
sparen INTEGER,
foreign key (nutzerid),
references user (nutzerid)
)