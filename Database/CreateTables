MariaDBServer 10.10.02

create table user
(
nutzerid INTEGER NOT NULL AUTO_INCREMENT,
name VARCHAR(255),
geburtsdatum DATE,
passwort INTEGER,
einkommen INTEGER,
PRIMARY KEY (nutzerid)
)

create table umsaetze
(
Datum DATE,
budgettype VARCHAR(255),
betrag INTEGER,
nutzerid INTEGER NOT NULL AUTO_INCREMENT,
beschreibung VARCHAR(255),
id INTEGER,
foreign key (id),
foreign key (nutzerid)
references user (nutzerid)
)

create table budget
(
nutzerid INTEGER NOT NULL AUTO_INCREMENT,
auto INTEGER,
wohnen INTEGER,
freizeit INTEGER,
essen INTEGER,
sparen INTEGER,
foreign key (nutzerid),
references user (nutzerid)
)