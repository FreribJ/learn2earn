create table budget
(
nutzer-id INTEGER NOT NULL AUTO_INCREMENT,
auto INTEGER,
wohnen INTEGER,
freizeit INTEGER,
essen INTEGER,
sparen INTEGER,
foreign key (nutzerid),
references user (nutzerid)
